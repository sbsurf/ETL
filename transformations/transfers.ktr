  <hop> <from>lead_assignments</from><to>contact lookup</to><enabled>Y</enabled> </hop>  <hop> <from>contact lookup</from><to>user lookup</to><enabled>Y</enabled> </hop>  <hop> <from>user lookup</from><to>Update contacts</to><enabled>Y</enabled> </hop>  </order>
  <step>
    <name>lead_assignments</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>cnb2</connection>
    <sql>SELECT * 
FROM lead_assignments t1
JOIN (SELECT lead_id, MAX(created_at) max_created_at FROM lead_assignments GROUP BY lead_id) t2
ON t1.lead_id = t2.lead_id AND t1.created_at = t2.max_created_at
JOIN users
ON t1.sales_rep_id = users.id AND users.enabled = 1
JOIN leads
ON t1.lead_id  = leads.id
ORDER BY t2.max_created_at;</sql>
