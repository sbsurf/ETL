    <connection>drm</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT DISTINCT contacts.id AS contact_id,
contacts.user_id AS contact_user_id,
campaign_components_contacts.id AS campaign_components_contacts_id

FROM contacts
LEFT JOIN addresses ON contacts.id = addresses.addressable_id
AND addresses.addressable_type = &quot;Contact&quot;
LEFT JOIN contact_companies ON contacts.id = contact_companies.contact_id
LEFT JOIN companies ON companies.id = contact_companies.company_id
LEFT JOIN campaign_components_contacts ON contacts.id = campaign_components_contacts.contact_id

WHERE soc = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
